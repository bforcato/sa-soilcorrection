package edu.utfpr.cp.dacom.sa.soilcorrection;

/**
 *
 * @author Beatriz
 */
public class CorrecaoFonteCalcio {
    CorrecaoCalcio Correcao = new CorrecaoCalcio();

    String SoloIdeal(int TextSolo){
 
        if ((TextSolo <= 2)||(TextSolo >= 1)){
            switch (TextSolo){
                case 1:
                    return "60 a 70%";
                case 2: 
                    return "50%";
            }
        }else
        return "";
    return null;
    }
}
